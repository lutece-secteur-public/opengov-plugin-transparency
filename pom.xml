<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

    <parent>
        <artifactId>lutece-global-pom</artifactId>
        <groupId>fr.paris.lutece.tools</groupId>
        <version>4.0.5</version>
    </parent>

    <modelVersion>4.0.0</modelVersion>
    <groupId>fr.paris.lutece.plugins</groupId>
        <artifactId>plugin-transparency</artifactId>
    <packaging>lutece-plugin</packaging>
    <version>1.0.4-SNAPSHOT</version>

		<name>Lutece transparency plugin</name>

     <repositories>
         <repository>
             <id>lutece</id>
             <name>luteceRepository</name>
             <url>http://dev.lutece.paris.fr/maven_repository</url>
             <layout>default</layout>
         </repository>
         <repository>
             <id>luteceSnapshot</id>
             <name>luteceSnapshot</name>
             <url>http://dev.lutece.paris.fr/snapshot_repository</url>
             <snapshots>
                 <enabled>true</enabled>
             </snapshots>
             <releases>
                 <enabled>false</enabled>
             </releases>
         </repository>        
     </repositories>

    <dependencies>
        <dependency>
            <groupId>fr.paris.lutece</groupId>
            <artifactId>lutece-core</artifactId>
            <version>[6.1.1,)</version>
            <type>lutece-core</type>
        </dependency>
        <dependency>
            <groupId>fr.paris.lutece.plugins</groupId>
            <artifactId>plugin-rest</artifactId>
            <version>[3.1.0,)</version>
            <type>lutece-plugin</type>
        </dependency>
        <dependency>
            <groupId>fr.paris.lutece.plugins</groupId>
            <artifactId>module-mylutece-database</artifactId>
            <version>5.0.0</version>
            <type>lutece-plugin</type>
        </dependency>
        <dependency>
            <groupId>fr.paris.lutece.plugins</groupId>
            <artifactId>library-httpaccess</artifactId>
            <version>[2.5.2-SNAPSHOT,)</version>
            <type>jar</type>
        </dependency>
    </dependencies>

    <properties>
        <componentName>transparency</componentName>
        <jiraProjectName>TRANSPARENCY</jiraProjectName>
        <jiraComponentId>TRANSPRNCY</jiraComponentId>
    </properties>
    
    
    
    <scm>
        <connection>scm:git:https://github.com/lutece-secteur-public/opengov-plugin-transparency</connection>
        <developerConnection>scm:git:https://github.com/lutece-secteur-public/opengov-plugin-transparency</developerConnection>
        <url>https://github.com/lutece-secteur-public/opengov-plugin-transparency</url>
        <tag>HEAD</tag>
    </scm>
    
</project>
